# Creative Automation Pipeline - Environment Configuration
# Copy this file to .env and add your actual API key

# Required: Google API key for Gemini 2.5 Flash Image (Nano Banana) generation
GOOGLE_API_KEY=your_key_here

# S3 Storage Configuration (optional - for cloud storage)
# If not set, the system will operate in local-only mode
S3_BUCKET_NAME=your-bucket-name
S3_REGION=us-east-1
S3_STORAGE_CLASS=STANDARD

# Optional: CloudFront CDN distribution for asset delivery
CLOUDFRONT_DISTRIBUTION_ID=your-distribution-id

# S3 Performance Tuning (optional)
S3_MAX_PARALLEL_UPLOADS=10
S3_ENABLE_VERSIONING=true
S3_ENABLE_ENCRYPTION=true
